{
  "name": "baret",
  "version": "1.0.0",
  "description": "Baret is a library that allows you to embed Bacon.js observables into React VDOM",
  "main": "lib/baret.js",
  "scripts": {
    "dist": "babel src --source-maps inline --out-dir lib",
    "lint": "eslint src test",
    "prepublish": "npm run lint && npm run test && npm run dist",
    "report-coverage": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "test": "nyc mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calmm-js/baret.git"
  },
  "keywords": [
    "jsx",
    "baconjs",
    "observables",
    "react",
    "reactive",
    "vdom"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/calmm-js/baret/issues"
  },
  "homepage": "https://github.com/calmm-js/baret#readme",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "codecov": "^1.0.1",
    "eslint": "^3.15.0",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-react": "^6.9.0",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "prop-types": "^15.5.8",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "dependencies": {
    "infestines": "^0.4.1",
    "baconjs": "^0.7",
    "react": "^15.0.1"
  }
}
